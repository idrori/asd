  "template": {
    "formatStyle": "neurips",
    "fontFamily": "Times",
    "fontSize": 10,
    "citationStyle": "ieee",
    "lineSpacing": 1.0,
    "marginInches": 1.0,
    "paperSize": "letter",
    "twoColumn": false,
    "anonymousSubmission": true,
    "styleFile": "NeurIPS 2025 LaTeX style file"
  },

  "review": {
    "criteriaFile": "review_criteria.txt",
    "minScoreForApproval": 4.0,
    "maxRevisionRounds": 1,
    "ratingScale": {
      "min": 1,
      "max": 6,
      "labels": {
        "6": "Strong Accept - Technically flawless, groundbreaking impact",
        "5": "Accept - Technically solid, high impact",
        "4": "Borderline Accept - Acceptance reasons outweigh rejection",
        "3": "Borderline Reject - Rejection reasons outweigh acceptance",
        "2": "Reject - Technical flaws, weak evaluation",
        "1": "Strong Reject - Well-known results or ethical concerns"
      }
    },
    "confidenceScale": {
      "min": 1,
      "max": 5,
      "labels": {
        "5": "Absolutely certain",
        "4": "Confident but not certain",
        "3": "Fairly confident",
        "2": "Willing to defend but gaps likely",
        "1": "Educated guess"
      }
    },
    "dimensionScale": {
      "min": 1,
      "max": 4,
      "labels": {
        "4": "Excellent",
        "3": "Good",
        "2": "Fair",
        "1": "Poor"
      }
    },
    "dimensions": [
      "quality",
      "clarity",
      "significance",
      "originality"
    ],
    "includeTrustworthiness": false
  },

  "prompts": {
    "builder": "builder.txt",
    "reviewer": "reviewer.txt",
