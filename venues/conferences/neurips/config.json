    "minPages": 9,
    "maxPages": 9,
    "cameraReadyMaxPages": 10,
    "referencesCountTowardLimit": false,
    "minReferences": 20,
    "maxReferences": 50
  },

  "paperStructure": {
    "sections": [
      "Abstract",
      "Introduction",
      "Related Work",
      "Problem Formulation",
      "Method",
      "Experiments",
      "Results",
      "Conclusion",
      "Paper Checklist",
      "References",
      "Appendix"
