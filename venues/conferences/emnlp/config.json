    "abstractRequired": true,
    "abstractMaxWords": 200,
    "keywordsRequired": false,
    "keywordsCount": { "min": 0, "max": 0 },
    "appendicesAllowed": true,
    "supplementaryAllowed": true,
    "limitationsRequired": true,
    "ethicsStatementOptional": true,
    "responsibleNLPChecklistRequired": true
  },

  "template": {
    "formatStyle": "acl",
    "fontFamily": "Times",
    "fontSize": 11,
    "citationStyle": "acl",
    "lineSpacing": 1.0,
    "marginInches": 1.0,
    "paperSize": "letter",
    "twoColumn": true,
    "anonymousSubmission": true,
    "styleFile": "https://github.com/acl-org/acl-style-files"
  },

  "review": {
    "criteriaFile": "review_criteria.txt",
    "minScoreForApproval": 3.5,
    "maxRevisionRounds": 1,
    "rollingReview": true,
    "dimensions": [
      "soundness",
      "significance",
      "novelty",
      "verifiability",
      "clarity",
      "limitations"
    ],
    "includeTrustworthiness": false
  },

  "prompts": {
    "builder": "builder.txt",
    "reviewer": "reviewer.txt",
