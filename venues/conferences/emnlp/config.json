  "paperLimits": {
    "minPages": 4,
    "maxPages": 8,
    "shortPaperMaxPages": 4,
    "longPaperMaxPages": 8,
    "cameraReadyExtraPage": 1,
    "referencesCountTowardLimit": false,
    "minReferences": 20,
    "maxReferences": 60
  },

  "paperStructure": {
    "sections": [
      "Abstract",
      "Introduction",
      "Related Work",
      "Method",
      "Experiments",
      "Results",
      "Analysis",
      "Limitations",
      "Ethics Statement",
      "Conclusion",
      "Acknowledgments",
      "References",
      "Appendix"
