    "maxRevisionRounds": 1,
    "ratingScale": {
      "min": 1,
      "max": 5,
      "labels": {
        "5": "Strong Accept",
        "4": "Accept",
        "3": "Weak Accept",
        "2": "Weak Reject",
        "1": "Reject"
      }
    },
    "confidenceScale": {
      "min": 1,
      "max": 5,
      "labels": {
        "5": "Absolutely certain",
        "4": "Confident but not certain",
        "3": "Fairly confident",
        "2": "Willing to defend but gaps likely",
        "1": "Educated guess"
      }
    },
    "dimensions": [
      "claimsAndEvidence",
      "priorWork",
      "originality",
      "significance",
      "clarity",
      "ethicalConsiderations"
    ],
    "includeTrustworthiness": false
  },

  "prompts": {
    "builder": "builder.txt",
    "reviewer": "reviewer.txt",
