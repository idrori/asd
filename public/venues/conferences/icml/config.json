  "paperStructure": {
    "sections": [
      "Abstract",
      "Introduction",
      "Related Work",
      "Preliminaries",
      "Method",
      "Theoretical Analysis",
      "Experiments",
      "Impact Statement",
      "Conclusion",
      "References",
      "Appendix"
