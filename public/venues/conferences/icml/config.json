    "abstractRequired": true,
    "abstractMaxWords": 200,
    "keywordsRequired": false,
    "keywordsCount": { "min": 0, "max": 0 },
    "appendicesAllowed": true,
    "supplementaryAllowed": true,
    "impactStatementRequired": true
